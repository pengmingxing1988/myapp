/**index.wxss**/
.index-banner{
  width:100%;
  height:200px;
  margin-bottom:20px;
}
.index-banner-item{
  width:100%;
  height:100%;
  position:relative;
}
.index-banner-item text{
  display:block;
  font-size:18px;
  line-height:30px;
  z-index:5;
  font-weight:bold;
  position:absolute;
  bottom:20px;
  color:#fff;
  padding:0px 10px;
  text-shadow:1px 1px 1px #000;
  text-align:center;
}